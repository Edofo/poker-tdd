{
  "name": "poker-final",
  "version": "1.0.0",
  "description": "A TDD poker project using Vitest",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "ci": "biome ci .",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@types/node": "^22.13.9",
    "@vitest/coverage-v8": "3.0.7",
    "@vitest/ui": "^3.0.7",
    "husky": "^9.1.7",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  }
}
